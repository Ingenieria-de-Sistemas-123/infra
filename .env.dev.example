# Archivo de ejemplo para entorno DEV (no credenciales reales)
# Copiar a .env.dev y reemplazar CHANGEME por valores reales

# --- Base de datos Snippet ---
DB_NAME=snippets_dev
DB_USERNAME=snippets_user_dev
DB_PASSWORD=CHANGEME_SNIPPETS_DEV

# --- Base de datos Permission ---
DB_PERMISSION_NAME=permission_dev
DB_PERMISSION_USER=permission_user_dev
DB_PERMISSION_PASSWORD=CHANGEME_PERMISSION_DEV

# JDBC URLs (valores expl√≠citos)
SPRING_DATASOURCE_URL_SNIPPET=jdbc:postgresql://snippet-db:5432/snippets_dev
SPRING_DATASOURCE_USERNAME_SNIPPET=snippets_user_dev
SPRING_DATASOURCE_PASSWORD_SNIPPET=CHANGEME_SNIPPETS_DEV

SPRING_DATASOURCE_URL_PERMISSIONDB=jdbc:postgresql://permission-db:5432/permission_dev
SPRING_DATASOURCE_USERNAME_PERMISSIONDB=permission_user_dev
SPRING_DATASOURCE_PASSWORD_PERMISSIONDB=CHANGEME_PERMISSION_DEV

# Perfil Spring
SPRING_PROFILES_ACTIVE=docker

# Base URL para language-service
LANGUAGE_BASE_URL=http://language-service:8080

# Asset service / Azurite
ASSET_SERVICE_AZURE_HOST=http://azurite
ASSET_SERVICE_NEW_RELIC_APP_NAME=asset-service-dev
ASSET_SERVICE_NEW_RELIC_AGENT_ENABLED=false

